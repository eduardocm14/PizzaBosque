  object fdqVendasProdutos: TFDQuery
    Connection = dmPrincipal.conPizzaria
    SQL.Strings = (
      'select '
      '  vp.id,'
      '  vp.numvenda,'
      '  vp.codproduto,'
      '  p.descricao,'
      '  p.valor'
      'from vendasprodutos vp'
      'inner join produtos p on p.id = vp.codproduto'
      'where vp.numvenda = :pVenda'
      'order by p.tipo desc')
    Left = 112
    Top = 88
    ParamData = <
      item
        Name = 'PVENDA'
        DataType = ftString
        ParamType = ptInput
        Value = Null
      end>
